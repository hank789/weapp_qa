/* 产品列表 */
.comment-product {
  padding: 32rpx 32rpx 0;}
.comment-product .product-info {
  overflow: hidden;
  border-radius: 8rpx;
  padding: 0 0 30rpx;
  background: none;}
.comment-product .product-info .product-img {
  width: 88rpx;
  height: 88rpx;
  float: left;}
.comment-product .product-img .logo-img {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  object-fit: contain;}
.comment-product .product-detail {
  float: left;
  margin-left: 20rpx;}
.comment-product .productName {
  width: 578rpx;
  color: #444444;
  font-size: 32rpx;
  line-height: 45rpx;}
.comment-product .productMark {
  margin-top: 10rpx;
  display: flex;}
.comment-product .productMark .starsText text {
  color: #B4B4B6;
  font-size: 22rpx;
  line-height: 30rpx;}
.comment-product .productMark .starsText text:nth-of-type(1) {
  margin-left: 6rpx;}


