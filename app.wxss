/**app.wxss**/
@import 'weui.wxss';
@import 'iconfont/iconfont.wxss';


/* 弹窗 */
.dialog {

}
.dialog.authentication .dialogContent .impowerUser {
  background: #03AEF9;
  border-color: #029FE4;
}
.dialog.authentication .dialogContent .wxLogo {
  width: 113rpx !important;
  height: 108rpx;
}
.dialog .mask {
  position: fixed;
  z-index: 10;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(0, 0, 0, 0.6);
}
.dialog .dialogContent {
  width: 600rpx;
  height: 447rpx;
  position: fixed;
  top: 50%;
  left: 50%;
  transform: translate(-50%, -50%);
  z-index: 50;
  background: #FFFFFF;
  border-radius: 36rpx;
}
.dialog .dialogContent .wxLogo {
  width: 108rpx;
  height: 108rpx;
  margin: 70rpx auto 0;
}
.dialog .dialogContent .wxLogo image {
  width: 100%;
  height: 100%;
}
.dialog .dialogContent .dialogText {
  text-align: center;
  color: #808080;
  font-size: 28rpx;
  line-height: 40rpx;
  margin: 48rpx 0 23rpx;
}
.dialog .dialogContent .impowerUser {
  width: 480rpx;
  height: 88rpx;
  line-height: 88rpx;
  background: #1AAD19;
  border: 2rpx solid #179E16;
  color: #FFFFFF;
  font-size: 32rpx;
}

.post-button {
  position: fixed;
  bottom: 88rpx;
  right: 50rpx;
  width:300rpx;
  line-height:88rpx;
  text-align:center;
  background:#1AAD19;
  color:#fff;
  -webkit-border-radius:100rpx;
  border-radisu:100rpx;
  left:50%;
  margin-left:-150rpx;
  z-index:5;
}
page {
    background-color: #fff;
    color: #333;
    font-size: 24rpx;
    box-sizing: border-box;
    padding-bottom: 20rpx;
}
.font-family-medium {
  font-family:PingFangSC-Medium;
}
.spot {
  width: 4rpx;
  height: 4rpx;
  position: relative;
  top: -6rpx;
  display: inline-block;
  border-radius: 100rpx;
  background: #B4B4B6;
  margin: 14rpx 18rpx 0;
}
.text-line-1 {
    overflow: hidden;
    white-space: nowrap;
    text-overflow: ellipsis;
}
.text-line-5 {
    display: -webkit-box;
    overflow: hidden;
    white-space: normal!important;
    text-overflow: ellipsis;
    word-wrap: break-word;
    -webkit-line-clamp: 5;
    -webkit-box-orient: vertical;
}
.line-river-big {
  width: 100%;
  height: 20rpx;
  background-color: #f3f4f6;
}
.line-river-after {
  position: relative;
  height: 1px; /* px不转换 */
  width: 100%; }
.line-river-after:after {
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  content: '';
  background-color: #dcdcdc;
  height: 1px; /* px不转换 */
  -webkit-transform: scaleY(0.5);
  transform: scaleY(0.5); }
.line-river-after-top {
  top: -1px; /* px不转换 */}
.bot {
  position: absolute;
  right: 0;
  bottom: 0;
  left: 0;
  height: 2rpx;
  -webkit-transform: scaleY(.5);
  transform: scaleY(.5);
  background-color: rgb(220, 220, 220);}
.container {
  height:80%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 0;
  box-sizing: border-box;
} 
.ellipsis {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:1
}
.ellipsis-2 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:2
}
.ellipsis-3 {
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp:3
}
.weui-loadmore__tips_in-line {
  background-color: #f2f2f2
}
.weui-loadmore {
  background-color: #fff;
  width: 100%;
  margin: 0 auto;
  padding: 20rpx;
  -webkit-box-sizing: border-box;
}
.touch-active:active {
  background-color: #f2f2f2
}
.weui-bg-transparent {
    background-color: transparent;
    width:85%;
    margin: 30rpx auto 0 auto;
    padding: 0
}

/* 产品列表 */
.comment-product {
  padding: 32rpx 32rpx 0;}
.comment-product .product-info {
  overflow: hidden;
  border-radius: 8rpx;
  padding: 0 0 30rpx;
  background: none;}
.comment-product .product-info .product-img {
  width: 88rpx;
  height: 88rpx;
  float: left;}
.comment-product .product-img .logo-img {
  width: 100%;
  height: 100%;
  border-radius: 8rpx;
  object-fit: contain;}
.comment-product .product-detail {
  float: left;
  margin-left: 20rpx;}
.comment-product .productName {
  width: 578rpx;
  color: #444444;
  font-size: 32rpx;
  line-height: 45rpx;}
.comment-product .productMark {
  display: flex;}
.comment-product .productMark text {
  color: #B4B4B6;
  font-size: 22rpx;
  /* float: left; */
  line-height: 30rpx;}
.comment-product .productMark text:nth-of-type(1) {
  color: #FCC816;
  margin-left: 6rpx;}


