<view class="page">
  <view class="page__bd">
      <view class="container-search">
        <view class="search-wrapper">
          <view class="iconfont icon-sousuo"></view>
          <input type="text" class="" placeholder-class="placeholder" placeholder="搜索你想了解的企业级产品和服务" value="{{inputVal}}" focus="{{inputShowed}}" bindinput="inputTyping" />
          <view class="close" wx:if="{{inputVal.length > 0}}" bindtap="clearInput">
            <view class="iconfont icon-times--"></view>
          </view>
        </view>
        <text class="cancel">取消</text>
      </view>
  </view>

