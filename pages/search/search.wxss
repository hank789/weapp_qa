page {
  background: #FFFFFF;
}
.page__bd {
  padding: 0 32rpx;
}
.container-search {
  position: relative;
  width:100%;
  display: flex;
  height:87.98rpx;
  margin-top: 15rpx;
  flex-direction: row;
  align-items: center;
  background: #ffffff;
  border-radius: 100rpx; 
  justify-content: space-between;
}
.container-search .search-wrapper {
  width: 568rpx;
  height: 68rpx;
  border-radius: 99.98rpx;
  background: #f3f4f6;
  display: flex;
  flex-direction: row;
  align-items: center;
  /* border: 2rpx solid #DCDCDC; */
  justify-content: space-between;
  padding: 0 19.96rpx;
}
.container-search .icon-sousuo {
  font-size: 34rpx;
  color: #C8C8C8;
  margin-top: 0rpx;
}
.container-search .close .icon-times {
  margin-left: 19.96rpx;
  font-size: 36rpx;
  position: relative;
  z-index: 10000;
  color: #C8C8C8;
}
.container-search input {
  width: 460rpx;
  height: 34rpx;
  border: none;
  background: #f3f4f6;
  font-size: 28rpx;
  color: #444444;
  position: absolute;
  left: 55rpx;
  z-index: 100000;
  padding: 0 19.96rpx;
}
.container-search .cancel {
  color: #03AEF9;
  font-size: 30rpx;
  font-family: PingFangSC-Medium;
}

.container-searchwrapper {
  padding: 0 32rpx;
  position: relative;
  z-index: 1000;
}
.search-list {
  color: #03AEF9;
  font-size: 30rpx;
  padding: 22rpx 0;
  line-height: 43.96rpx;
  position: relative;
}
.container-searchwrapper .search-list.listOne {
  margin-top: 12rpx;
}
.container-searchwrapper .search-list .gray {
  color: #C8C8C8;
}
.stars {
  width: 152rpx;
  position: relative;
}
.stars .icon-xingxingyidian {
  font-size: 115rpx;
  position: absolute;
  color: #fcc816;
  top: -72rpx;
  overflow: hidden;
}
.starsText .spot {
  margin: 14rpx 10rpx 0;
}
.docbutton {
  background: #FFFFFF;
}
.docbutton::after {
  border: none;
}
