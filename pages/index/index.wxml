<view class="page">
	<view class="page__hd">
	    <view class="ask-title">
		    <image class="ask-title-avatar" src="../../images/logo-144.png" background-size="cover"></image>
	    </view>
	</view>

    <view class="page__bd" bindtap="inputTyping">
        <view class="weui-search-bar-custom">
            <view class="weui-search-bar__form">
                <view class="weui-search-bar__box">
                    <icon class="weui-icon-search_in-box" type="search" size="14"></icon>
                    <input type="text" disabled="true" class="weui-search-bar__input" placeholder="输入您的问题" />
                </view>
            </view>
        </view>
    </view>
</view>
