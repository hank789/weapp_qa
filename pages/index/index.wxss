      top: -51rpx;
      opacity: 0;
      visibility: hidden;
    }
    100% {
      top: -51rpx;
      opacity: 0;
      visibility: hidden;
    }
  }
  @-webkit-keyframes dowmShadow {
    0% {
      top: -37rpx;
      opacity: 0;
    }
    10% {
      top: -41rpx;
      opacity: 1;
    }
    90% {
      top: -51rpx;
