.mui-content {
  background: #ffffff;
}
.titleText {
  color: #808080;
  font-size: 27.98rpx;
  padding: 0 31.96rpx;
  line-height: 39.98rpx;
}
.feedBackWrapper {
  padding: 0 31.96rpx;
}
.feedBackWrapper .title {
  padding: 24rpx 0;
  position: relative;
  overflow: hidden;
}
.feedBackWrapper .title.active text {
  color: #C8C8C8;
}
.feedBackWrapper .title.active .icon {
  display: block;
  color: #03AEF9;
}
.feedBackWrapper .title text {
  color: #444444;
  font-size: 27.98rpx;
  line-height: 39.98rpx;
  float: left;
}
.feedBackWrapper .title .icon {
  float: right;
  font-size: 33.98rpx;
  color: #03AEF9;
  display: none;
}
.productSummary {
  padding-top: 21.98rpx;
  padding-bottom: 24rpx;
}
.productSummary .textareaWrapper {
  padding: 25.96rpx 31.96rpx 0;
}
.productSummary .textareaWrapper textarea {
  width: 626rpx;
  padding: 24rpx 30rpx;
  height: 280rpx;
  color: #444444;
  font-size: 27.98rpx;
  line-height: 39.98rpx;
  border: 1rpx solid #DCDCDC;
  border-radius: 10rpx;
}
.pictureCaption .container-images {
  margin-top: 24rpx;
}
.sureButton {
  margin: 19.96rpx auto 30rpx;
  width: 685.96rpx;
  height: 87.98rpx;
  color: #FFFFFF;
  font-size: 31.96rpx;
  line-height: 87.98rpx;
  text-align: center;
  background: #03AEF9;
  border-radius: 199.96rpx;
  font-family: PingFangSC-Medium;
}
.component-photograph {
  width: 121.96rpx !important;
  height: 121.96rpx !important;
}

.container-images .container-image {
  margin: 0 20rpx 20rpx 0;
  float: left;
  width: 122rpx;
  height: 122rpx;
  border-radius: 8rpx;
  position: relative;
}
.container-images .container-image:nth-of-type(3n+1) {
  clear: both;
}
.container-image .iconfont {
  position: absolute;
  right: -10rpx;
  top: -10rpx;
}
.container-image image {
  border-radius: 10rpx;
  width: 100%;
  height: 100%;
  -o-object-fit: contain;
  object-fit: contain;
}