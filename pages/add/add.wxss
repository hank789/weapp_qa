.component-mark {
  padding: 15.98rpx 31.96rpx 19.96rpx;
}
span {
  color: #B4B4B6;
  font-size: 24rpx;
  line-height: 33.0rpx;
}
.stars {
  position: relative;
  color: #FCC816;
  font-size: 45.98rpx;
  /*margin-top: 15.98rpx;*/
  margin-bottom: 30rpx;
}
.ratingNumber {
  position: absolute;
  right: 0;
  bottom: 3.98rpx;
  font-size: 36rpx;
}

.container-identity {
  padding-bottom:19.96rpx;}
.line-river-after:after {
  left: 31.96rpx;
  right: 31.96rpx;
}
.identityTitle {
  padding: 24rpx 31.96rpx 21.98rpx;
  line-height: 42rpx;
  display: flex;
  justify-content: space-between;}
.identityTitle text {
  color: #444444;
  font-size: 30rpx;
}
.identityTitle .iconfont {
  color: #808080;
  margin-top: 6rpx;
}
.assessDomain {
  color: #B4B4B6;
  font-size: 24rpx;
  padding: 24.98rpx 31.96rpx 12rpx;
  line-height: 33.0rpx;
}
.domainList {
  padding: 0 31.96rpx;
}
.domainList text {
  color: #B4B4B6;
  font-size: 24rpx;
  line-height: 33.0rpx;
  padding: 9.98rpx 21.98rpx;
  margin-right: 19.96rpx;
  margin-top: 12.98rpx;
  display: inline-block;
  border-radius: 199.96rpx;
}

.component-photograph {
  width: 121.96rpx !important;
  height: 121.96rpx !important;
  /* float: left; */
  margin-bottom: 19.96rpx;
  display: inline-block;
  border-radius: 8rpx;
  background: #dcdcdc;
  position: relative;
}
.component-photograph .iconfont {
  position: absolute;
  color: #fff;
  font-size: 56rpx;
  top: 50%;
  left: 50%;
  margin-left: -28rpx;
  margin-top: -28rpx;
}
.container-images .container-image {
  margin: 0 20rpx 20rpx 0;
  float: left;
  width: 122rpx;
  height: 122rpx;
  border-radius: 8rpx;
  position: relative;
}
.container-images .container-image:nth-of-type(3n+1) {
  clear: both;
}
.container-image .iconfont {
  position: absolute;
  right: -10rpx;
  top: -10rpx;
}
.container-image image {
  border-radius: 10rpx;
  width: 100%;
  height: 100%;
  -o-object-fit: contain;
  object-fit: contain;
}