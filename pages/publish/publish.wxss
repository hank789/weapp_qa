.sucessWrapper {
  text-align: center;
  margin-top: 188rpx;
}
.sucessWrapper .icon-check-circle {
  font-size: 248rpx;
  color: #F3F4F6;
  margin: -77rpx 0;
}
.sucessWrapper .sucessText {
  font-size: 40rpx;
  color: #323232;
  line-height: 48rpx;
  margin-top: 30rpx;
}
.sucessWrapper .share {
  color: #808080;
  font-size: 28rpx;
  line-height: 40rpx;
  margin-top: 20rpx;
}
.sucessWrapper .shareIcon {
  width: 88rpx;
  height: 88rpx;
  margin: 0 auto;
  border-radius: 50%;
  background: #FFFFFF;
  position: absolute;
  top: 0;
  left: 0;
}
.sucessWrapper .shareIcon .iconfont {
  color: #03AEF9;
  font-size: 43rpx;
  line-height: 88rpx;
}
.docbutton {
  width: 88rpx;
  height: 88rpx;
  margin: 40rpx auto;
  border-radius: 50%;
  position: relative;
  box-shadow:0px 0px 19rpx 0px rgba(234,240,244,1);
  background: #FFFFFF;
}
button.docbutton::after {
  border: none;
}

.myComment {
  position: absolute;
  bottom: 80rpx;
  display: flex;
  left: 50%;
  transform: translateX(-50%);
}
.myComment .editImg {
  width: 34rpx;
  height: 35rpx;
  margin-right: 10rpx;
}
.myComment .editImg .img {
  width: 100%;
  height: 100%;
}
.myComment .myCommentText {
  color: #03AEF9;
  font-size: 24rpx;
}