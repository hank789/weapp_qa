<!--拉起微信授权 模版-->
  <view class='dialog authentication' hidden='{{ !isHidden }}' catchtouchmove='true'>
    <view class='mask'></view>
    <view class='dialogContent'>
      <view class='close' bindtap='classPopup'>
        <view class='iconfont icon-times--'></view>
      </view>
      <view class='wxLogo logotop'>
        <image src='../../images/page_2@3x.png'></image>
      </view>
      <view class='dialogText'>
        <text>依网络法律要求，请完成手机实名认证</text>
      </view>
      <button class='impowerUser' open-type='getPhoneNumber' bindgetphonenumber="getUserPhone">一键认证</button>
    </view>
  </view>
